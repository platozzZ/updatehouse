.cu-list.menu>.cu-item::after{
  transform: scale(1);
  width: calc(100% - 30rpx);
  height: 100%;
  left: 30rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.cu-list.menu>.cu-item .content{
  width: calc(100vw - 120rpx);
  line-height: auto
}
.emptyContainer{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  color: #999;
}
